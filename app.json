{
    "name": "Trash-Core",
    "description": "A whatsApp bot based on Baileysjs linrary (Ts+Js) with Multi Device support",
    "repository": "https://github.com/anonphoenix007/CH13",
    "logo": "https://telegra.ph/file/7aa528767ed96ddc17bc2.jpg",
    "keywords": ["nodejs", "bot", "whatsapp bot", "whatsapp automation", "multi device"],
    "env": {
    "OWNER_NAME": {
      "description": "enter your owner name here",
      "value": "Take Tee",
      "required": true
    },
    "BOT_PREFIX": {
      "description": "Put your bot prefix of desire here",
      "value": ".",
      "required": false
    },
   "ANTIDELETE": {
      "description": "type true if you want your bot to detect deleted messages in your dm if else type false",
      "value": "true",
      "required": false
    },
   "AUTO_STATUS": {
      "description": "type true if you want bot to view status if else type false",
      "value": "true",
      "required": false
    },
    "PACK_NAME": {
      "description": "type your sticker pack name here",
      "value": "Trashcore pack",
      "required": false
    },
      "API_KEY": {
      "description": "insert your heroku api key here ",
      "value": "",
      "required": true
    },
    "AUTHOR": {
      "description": "type your author sticker name here",
      "value": "Trashcore",
      "required": false
    },
    "SESSION_ID": {
      "description": "put your session ID here",
      "value": "trashcore~",
      "required": true
    }
    },
    "buildpacks": [
      {
        "url": "heroku/nodejs"
    },
    {
